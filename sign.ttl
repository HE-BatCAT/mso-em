##########################
# Highest-level concepts #
##########################

mso-em:Articulation a owl:Class;
   skos:prefLabel "Articulation"@en;
   rdfs:label     "Articulation"@en;
   skos:altLabel "Articulation"@en;
   rdfs:subClassOf dol:endurant, [
      a owl:Restriction;
      owl:onProperty dol:generic-constituent;
      owl:someValuesFrom dol:endurant
   ];
   skos:exactMatch pims-ii:Articulation;
   skos:definition "Articulation (collective expression), i.e., a sign (type/collection) the realizations of which are not collective expressions, but individual literal (written or digital) or non-literal (e.g., spoken) items with the same semiotic function."@en;
   rdfs:comment    "Articulation (collective expression), i.e., a sign (type/collection) the realizations of which are not collective expressions, but individual literal (written or digital) or non-literal (e.g., spoken) items with the same semiotic function."@en.

pims-ii:Plurality a owl:Class;
   skos:prefLabel "Plurality"@en;
   rdfs:label     "Plurality"@en;
   skos:altLabel "Plurality"@en;
   rdfs:subClassOf dol:set, [
      a owl:Restriction;
      owl:onProperty dol:generic-constituent;
      owl:someValuesFrom dol:endurant
   ];
   skos:exactMatch pims-ii:Plurality;
   skos:definition "Plurality, i.e., a semiotic collective the members of which are a representational element (referent or representamen) together, whereby all act/contribute in the same way"@en;
   rdfs:comment "Plurality, i.e., a semiotic collective the members of which are a representational element (referent or representamen) together, whereby all act/contribute in the same way"@en;
   skos:example "The referent of the expression 'the four seasons/quarters of the year' (for a given year, within a given spatial region) is a plurality, and each of the four seasons/quarters is one of its semiotic members"@en.
