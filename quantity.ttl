   owl:versionIRI <https://batcat.info/semantics/mso-em-2025-06/quantity.ttl>;
   dcterms:title "MSO-EM module 'quantity' (Equality, Term, Unit)"@en;
   dcterms:created "2025"^^dcterms:W3CDTF;
   owl:versionInfo "MSO-EM module 'quantity': Version dated June 2025"@en;
