@base <https://www.purl.org/mso-em/domain/tqc#>.
@prefix tqc: <https://www.purl.org/mso-em/domain/tqc#>.

@prefix dcelem: <http://purl.org/dc/elements/1.1/>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix thequo: <https://www.purl.org/mso-em/domain/thequo#>.
@prefix xs: <http://www.w3.org/2001/XMLSchema#>.

<https://www.purl.org/mso-em/domain/tqc> a owl:Ontology;
   owl:imports <https://batcat.info/semantics/mso-em/domain/thequo.ttl>;
   dcterms:title "Thermodynamic quality catalogue"^^xs:string;
   dcterms:created "2025"^^dcterms:W3CDTF;
   owl:versionInfo "Version dated October 2025"^^xs:string;
   dcterms:issued "2025-10-16"^^dcterms:W3CDTF;
   dcterms:creator
      <https://orcid.org/0000-0003-0422-7870>,  # Silvia Chiacchiera
      <https://orcid.org/0000-0002-9464-6739>,  # Martin Thomas Horsch
      <https://orcid.org/0000-0002-4578-3569>.  # Simon Stephan

#########################
# Legacy implementation #
#########################

tqc:U a owl:NamedIndividual,
        thequo:ThermodynamicPotentialFundamentalForm ;
    thequo:hasFundamentalDerivative <https://batcat.info/semantics/example#-p>,
        tqc:T ;
    thequo:hasNaturalVariablesSet tqc:SV-NaturalVariablesSet ;
    thequo:hasPartAddendum <https://batcat.info/semantics/example#-pV>,
        tqc:TS .

tqc:dU a owl:NamedIndividual,
        thequo:TotalDifferential ;
    thequo:hasPartAddendum <https://batcat.info/semantics/example#-pdV>,
        tqc:TdS .

tqc:hasPart a owl:ObjectProperty ;
    rdfs:label "has part" .

tqc:isDirectlyFollowedBy a owl:ObjectProperty ;
    rdfs:label "is directly followed by" .


tqc:SV-NaturalVariablesSet a owl:NamedIndividual ;
    tqc:hasPart tqc:S,
        tqc:V .

tqc:TS a owl:NamedIndividual ;
    thequo:hasPartFactor tqc:S,
        tqc:T .

tqc:TdS a owl:NamedIndividual ;
    thequo:hasPartFactor tqc:T,
        tqc:dS .

tqc:dS a owl:NamedIndividual .

tqc:dV a owl:NamedIndividual,
        thequo:GeneralizedDisplacement .

tqc:S a owl:NamedIndividual ;
    tqc:isDirectlyFollowedBy tqc:V .

tqc:T a owl:NamedIndividual,
        thequo:FundamentalDerivative ;
    thequo:hasDecompositionVector "(1,0)"^^xs:datatype .

tqc:V a owl:NamedIndividual .
