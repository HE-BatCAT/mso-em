#########################
# Legacy implementation #
#########################

thequo:NaturalVariableSet a owl:Class ;
    rdfs:label "Natural Variable Set" .

thequo:Product a owl:Class ;
    rdfs:label "Product" .

thequo:Sum a owl:Class ;
    rdfs:label "Sum" .

thequo:ThermodynamicPotential a owl:Class ;
    rdfs:label "Thermodynamic Potential" .

thequo:ThermodynamicSystem a owl:Class ;
    rdfs:label "Thermodynamic System" .

thequo:hasDecompositionVector a owl:DatatypeProperty ;
    rdfs:label "has decomposition vector" .

thequo:hasFundamentalDerivative a owl:ObjectProperty ;
    rdfs:label "has fundamental derivative" .

thequo:hasNaturalVariableSet a owl:ObjectProperty ;
    rdfs:label "has natural variable set" .

thequo:hasNumberOfNaturalVariables a owl:DatatypeProperty ;
    rdfs:label "has number of natural variables" .

thequo:hasPartAddendum a owl:ObjectProperty ;
    rdfs:label "has part addendum" .

thequo:hasPartFactor a owl:ObjectProperty ;
    rdfs:label "has part factor" .

thequo:hasPhysicalQuality a owl:ObjectProperty ;
    rdfs:label "has physical quality" .

thequo:ExtensivePhysicalQuality a owl:Class ;
    rdfs:label "Extensive Physical Quality" ;
    rdfs:subClassOf thequo:PhysicalQuality ;
    owl:disjointWith thequo:IntensivePhysicalQuality .

thequo:GeneralizedDisplacement a owl:Class ;
    rdfs:label "Generalized Displacement" ;
    rdfs:subClassOf thequo:TotalDifferential .

thequo:GeneralizedForce a owl:Class ;
    rdfs:label "Generalized Force" ;
    rdfs:subClassOf thequo:IntensivePhysicalQuality .

thequo:ThermodynamicPotentialExpression a owl:Class ;
    rdfs:label "Thermodynamic Potential Expression" .

thequo:ThermodynamicPotentialFundamentalForm a owl:Class ;
    rdfs:label "Thermodynamic Potential Fundamental Form" ;
    rdfs:subClassOf thequo:ThermodynamicPotentialExpression .

thequo:FundamentalDerivative a owl:Class ;
    rdfs:label "Fundamental Derivative" .

thequo:IntensivePhysicalQuality a owl:Class ;
    rdfs:label "Intensive Physical Quality" ;
    rdfs:subClassOf thequo:PhysicalQuality ;
    owl:disjointWith thequo:ExtensivePhysicalQuality .

thequo:PhysicalQuality a owl:Class ;
    rdfs:label "Physical Quality" .

thequo:TotalDifferential a owl:Class ;
    rdfs:label "Total Differential" .

