@base <https://www.purl.org/mso-em/domain/thequo#>.
@prefix thequo: <https://www.purl.org/mso-em/domain/thequo#>.

@prefix dcelem: <http://purl.org/dc/elements/1.1/>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix dol: <http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#>.
@prefix mso-em: <https://www.purl.org/mso-em#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix tqc: <https://www.purl.org/mso-em/domain/tqc#>.
@prefix xs: <http://www.w3.org/2001/XMLSchema#>.

<https://www.purl.org/mso-em/domain/thequo> a owl:Ontology;
   dcterms:title "Thermodynamic quality ontology"^^xs:string;
   dcterms:created "2025"^^dcterms:W3CDTF;
   owl:versionInfo "Version dated October 2025"^^xs:string;
   dcterms:issued "2025-10-14"^^dcterms:W3CDTF;
   dcterms:creator
      <https://orcid.org/0000-0003-0422-7870>,  # Silvia Chiacchiera
      <https://orcid.org/0000-0002-9464-6739>,  # Martin Thomas Horsch
      <https://orcid.org/0000-0002-4578-3569>.  # Simon Stephan

tqc:U a owl:NamedIndividual,
        thequo:ThermodynamicPotentialFundamentalForm ;
    thequo:hasFundamentalDerivative <https://batcat.info/semantics/example#-p>,
        tqc:T ;
    thequo:hasNaturalVariablesSet tqc:SV-NaturalVariablesSet ;
    thequo:hasPartAddendum <https://batcat.info/semantics/example#-pV>,
        tqc:TS .

tqc:dU a owl:NamedIndividual,
        thequo:TotalDifferential ;
    thequo:hasPartAddendum <https://batcat.info/semantics/example#-pdV>,
        tqc:TdS .

tqc:hasPart a owl:ObjectProperty ;
    rdfs:label "has part" .

tqc:isDirectlyFollowedBy a owl:ObjectProperty ;
    rdfs:label "is directly followed by" .


thequo:Addendum a owl:Class ;
    rdfs:label "Addendum" .

thequo:Factor a owl:Class ;
    rdfs:label "Factor" .

thequo:NaturalVariablesSet a owl:Class ;
    rdfs:label "Natural Variables Set" .

thequo:Product a owl:Class ;
    rdfs:label "Product" .

thequo:Sum a owl:Class ;
    rdfs:label "Sum" .

thequo:ThermodynamicPotential a owl:Class ;
    rdfs:label "Thermodynamic Potential" .

thequo:ThermodynamicSystem a owl:Class ;
    rdfs:label "Thermodynamic System" .

thequo:hasDecompositionVector a owl:DatatypeProperty ;
    rdfs:label "has decomposition vector" .

thequo:hasFundamentalDerivative a owl:ObjectProperty ;
    rdfs:label "has fundamental derivative" .

thequo:hasNaturalVariablesSet a owl:ObjectProperty ;
    rdfs:label "has natural variables set" .

thequo:hasNumberOfNaturalVariables a owl:DatatypeProperty ;
    rdfs:label "has number of natural variables" .

thequo:hasPartAddendum a owl:ObjectProperty ;
    rdfs:label "has part addendum" .

thequo:hasPartFactor a owl:ObjectProperty ;
    rdfs:label "has part factor" .

thequo:hasPhysicalQuality a owl:ObjectProperty ;
    rdfs:label "has physical quality" .

<https://batcat.info/semantics/example#-pV> a owl:NamedIndividual ;
    thequo:hasPartFactor <https://batcat.info/semantics/example#-p>,
        tqc:V .

<https://batcat.info/semantics/example#-pdV> a owl:NamedIndividual ;
    thequo:hasPartFactor <https://batcat.info/semantics/example#-p>,
        tqc:dV .

tqc:SV-NaturalVariablesSet a owl:NamedIndividual ;
    tqc:hasPart tqc:S,
        tqc:V .

tqc:TS a owl:NamedIndividual ;
    thequo:hasPartFactor tqc:S,
        tqc:T .

tqc:TdS a owl:NamedIndividual ;
    thequo:hasPartFactor tqc:T,
        tqc:dS .

tqc:dS a owl:NamedIndividual .

tqc:dV a owl:NamedIndividual,
        thequo:GeneralizedDisplacement .

thequo:ExtensivePhysicalQuality a owl:Class ;
    rdfs:label "Extensive Physical Quality" ;
    rdfs:subClassOf thequo:PhysicalQuality ;
    owl:disjointWith thequo:IntensivePhysicalQuality .

thequo:GeneralizedDisplacement a owl:Class ;
    rdfs:label "Generalized Displacement" ;
    rdfs:subClassOf thequo:TotalDifferential .

thequo:GeneralizedForce a owl:Class ;
    rdfs:label "Generalized Force" ;
    rdfs:subClassOf thequo:IntensivePhysicalQuality .

thequo:ThermodynamicPotentialExpression a owl:Class ;
    rdfs:label "Thermodynamic Potential Expression" .

thequo:ThermodynamicPotentialFundamentalForm a owl:Class ;
    rdfs:label "Thermodynamic Potential Fundamental Form" ;
    rdfs:subClassOf thequo:ThermodynamicPotentialExpression .

tqc:S a owl:NamedIndividual ;
    tqc:isDirectlyFollowedBy tqc:V .

thequo:FundamentalDerivative a owl:Class ;
    rdfs:label "Fundamental Derivative" .

thequo:IntensivePhysicalQuality a owl:Class ;
    rdfs:label "Intensive Physical Quality" ;
    rdfs:subClassOf thequo:PhysicalQuality ;
    owl:disjointWith thequo:ExtensivePhysicalQuality .

thequo:PhysicalQuality a owl:Class ;
    rdfs:label "Physical Quality" .

thequo:TotalDifferential a owl:Class ;
    rdfs:label "Total Differential" .

<https://batcat.info/semantics/example#-p> a owl:NamedIndividual,
        thequo:FundamentalDerivative,
        thequo:GeneralizedForce ;
    thequo:hasDecompositionVector "(0,1)"^^xs:datatype .

tqc:T a owl:NamedIndividual,
        thequo:FundamentalDerivative ;
    thequo:hasDecompositionVector "(1,0)"^^xs:datatype .

tqc:V a owl:NamedIndividual .

